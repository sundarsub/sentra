{
  "name": "python_sandbox_v1",
  "description": "Restricted syscall profile for Python sandboxing",
  "default": "allow",
  "deny": [
    "execve",
    "execveat",
    "socket",
    "connect",
    "bind",
    "listen",
    "accept",
    "accept4",
    "sendto",
    "recvfrom",
    "sendmsg",
    "recvmsg",
    "unlink",
    "unlinkat",
    "rmdir",
    "rename",
    "renameat",
    "renameat2",
    "chmod",
    "fchmod",
    "fchmodat",
    "chown",
    "fchown",
    "fchownat",
    "ptrace",
    "mount",
    "umount2",
    "pivot_root",
    "bpf",
    "perf_event_open",
    "init_module",
    "delete_module",
    "keyctl",
    "unshare",
    "setns",
    "setuid",
    "setgid",
    "setresuid",
    "setresgid",
    "setgroups",
    "capset",
    "reboot",
    "kexec_load",
    "swapon",
    "swapoff",
    "sethostname"
  ]
}
